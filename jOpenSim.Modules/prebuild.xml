<?xml version="1.0" encoding="utf-8" ?>
<Prebuild xmlns="http://dnpb.sourceforge.net/schemas/prebuild-1.10.xsd" version="1.10">
  <Solution name="jOpenSim.Modules" activeConfig="Release" path="./" version="0.5.0-$Rev$">
    <Configuration name="Debug">
      <Options>
        <CompilerDefines>TRACE;DEBUG</CompilerDefines>
        <OptimizeCode>false</OptimizeCode>
        <CheckUnderflowOverflow>false</CheckUnderflowOverflow>
        <AllowUnsafe>false</AllowUnsafe>
        <WarningLevel>4</WarningLevel>
        <WarningsAsErrors>false</WarningsAsErrors>
        <SuppressWarnings>SYSLIB0014</SuppressWarnings>
        <OutputPath>bin</OutputPath>
        <DebugInformation>true</DebugInformation>
        <IncrementalBuild>true</IncrementalBuild>
        <NoStdLib>false</NoStdLib>
      </Options>
    </Configuration>
    <Configuration name="Release">
      <Options>
        <CompilerDefines>TRACE</CompilerDefines>
        <OptimizeCode>true</OptimizeCode>
        <CheckUnderflowOverflow>false</CheckUnderflowOverflow>
        <AllowUnsafe>false</AllowUnsafe>
        <WarningLevel>4</WarningLevel>
        <WarningsAsErrors>false</WarningsAsErrors>
        <SuppressWarnings>SYSLIB0014</SuppressWarnings>
        <OutputPath>bin</OutputPath>
        <DebugInformation>false</DebugInformation>
        <IncrementalBuild>true</IncrementalBuild>
        <NoStdLib>false</NoStdLib>
      </Options>
    </Configuration>

    <Project frameworkVersion="v8_0" name="jOpenSim.Profile" path="jOpenSim.Profile" type="Library">
      <Configuration name="Debug">
        <Options>
          <OutputPath>../bin/</OutputPath>
        </Options>
      </Configuration>
      <Configuration name="Release">
        <Options>
          <OutputPath>../bin/</OutputPath>
        </Options>
      </Configuration>

      <ReferencePath>../../bin/</ReferencePath>
      <Reference name="Nini.dll" path="../../bin/"/>
      <Reference name="log4net.dll" path="../../bin/"/>
      <Reference name="XMLRPC.dll" path="../../bin/"/>
      <Reference name="Mono.Addins.dll" path="../../bin/"/>
      <Reference name="OpenMetaverseTypes.dll" path="../../bin/"/>
      <Reference name="OpenMetaverse.dll" path="../../bin/"/>
      <Reference name="OpenSim.Region.Framework" path="../../bin/"/>
      <Reference name="OpenSim.Framework" path="../../bin/"/>
      <Reference name="OpenSim.Services.Interfaces" path="../../bin/"/>

      <Files>
        <Match pattern="*.cs" recurse="true"/>
      </Files>
    </Project>


    <Project frameworkVersion="v8_0" name="jOpenSim.Search" path="jOpenSim.Search" type="Library">
      <Configuration name="Debug">
        <Options>
          <OutputPath>../bin/</OutputPath>
        </Options>
      </Configuration>
      <Configuration name="Release">
        <Options>
          <OutputPath>../bin/</OutputPath>
        </Options>
      </Configuration>

      <ReferencePath>../../bin/</ReferencePath>

      <Reference name="Nini.dll" path="../../bin/"/>
      <Reference name="log4net.dll" path="../../bin/"/>
      <Reference name="XMLRPC.dll" path="../../bin/"/>
      <Reference name="Mono.Addins.dll" path="../../bin/"/>
      <Reference name="OpenMetaverseTypes.dll" path="../../bin/"/>
      <Reference name="OpenMetaverse.dll" path="../../bin/"/>
      <Reference name="OpenSim.Framework" path="../../bin/"/>
      <Reference name="OpenSim.Framework.Console" path="../../bin/"/>
      <Reference name="OpenSim.Framework.Monitoring" path="../../bin/"/>
      <Reference name="OpenSim.Region.Framework" path="../../bin/"/>
      <Reference name="OpenSim.Region.CoreModules" path="../../bin/"/>

      <Reference name="OpenSim.Services.Interfaces" path="../../bin/"/>

      <Files>
        <Match pattern="*.cs" recurse="true"/>
      </Files>
    </Project>


  </Solution>
</Prebuild>

